# Ejercicio 6.2

class Alumno():
    def inicializar(self, nombre, nota) :
        self.nombre = nombre
        self.nota = nota
    
    def imprimir(self):
        print("Nombre: ", self.nombre)
        print("Nota: ", self.nota)

    def resultado(self):
        if self.nota <= 5:
            print("Curso reprobado")
        else:
            print("Curso aprobado")
        
alumno = Alumno()
alumno.inicializar("Laura", 4)
alumno.imprimir()
alumno.resultado()
